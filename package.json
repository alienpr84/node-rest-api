{
  "name": "node-rest-api",
  "version": "1.0.0",
  "description": "This is a node rest api app for test js frameworks",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=dev nodemon -L src/index.ts",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --silent",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alienpr84/node-rest-api.git"
  },
  "keywords": [
    "node",
    "rest-api",
    "api",
    "express",
    "test",
    "testing",
    "typescript",
    "mongoose",
    "docker",
    "docker-compose",
    "joi",
    "mongodb"
  ],
  "author": "Alien Padilla Rodriguez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alienpr84/node-rest-api/issues"
  },
  "homepage": "https://github.com/alienpr84/node-rest-api#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.5.3",
    "@types/jest": "^26.0.23",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.10.5",
    "@types/nodemon": "^1.19.0",
    "@types/supertest": "^2.0.11",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "joi": "^17.4.0",
    "mongoose": "^5.12.6"
  }
